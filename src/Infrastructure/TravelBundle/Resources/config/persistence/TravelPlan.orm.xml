<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
<entity name="App\Domain\Travel\Model\TravelPlan" table="travelPlan">
<id name="id" type="integer" column="id">
        <generator strategy="IDENTITY"/>
    </id>
    <field name="createdAt" type="datetime" column="createdAt" precision="0" scale="0" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updatedAt" precision="0" scale="0" nullable="false"/>
    <field name="title" column="title" length="255" precision="0" scale="0" nullable="false"/>
    <field name="slug" column="slug" length="128" precision="0" scale="0" unique="true" nullable="false"/>
    <field name="photo"  column="photo" length="255" precision="0" scale="0" nullable="true"/>
    <field name="lat" type="decimal" column="lat" precision="14" scale="8" nullable="false"/>
    <field name="lng" type="decimal" column="lng" precision="14" scale="8" nullable="false"/>
    <field name="lat0" type="decimal" column="lat0" precision="14" scale="8" nullable="true"/>
    <field name="lng0" type="decimal" column="lng0" precision="14" scale="8" nullable="true"/>
    <field name="lat1" type="decimal" column="lat1" precision="14" scale="8" nullable="true"/>
    <field name="lng1" type="decimal" column="lng1" precision="14" scale="8" nullable="true"/>
    <field name="startAt" type="datetime" column="startAt" precision="0" scale="0" nullable="false"/>
    <field name="endAt" type="datetime" column="endAt" precision="0" scale="0" nullable="false"/>
    <field name="description" type="text" column="description" precision="0" scale="0" nullable="true"/>
    <field name="starts" type="integer" column="starts" precision="0" scale="0" nullable="true"/>
    <field name="watch" type="integer" column="watch" precision="0" scale="0" nullable="true"/>
    <one-to-many field="gpx" target-entity="App\Domain\Gpx\Model\Gpx" mapped-by="travel" fetch="LAZY">
        <cascade>
            <cascade-persist/>
        </cascade>
    </one-to-many>
