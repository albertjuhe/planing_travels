 <one-to-many field="location" target-entity="App\Entity\Location" mapped-by="travel" fetch="LAZY">
     <cascade>
         <cascade-persist/>
     </cascade>
 </one-to-many>
 <many-to-one field="user" target-entity="App\Entity\User" inversed-by="travel" fetch="LAZY">
     <join-columns>
         <join-column name="user_id" referenced-column-name="id" nullable="1"/>
     </join-columns>
 </many-to-one>
