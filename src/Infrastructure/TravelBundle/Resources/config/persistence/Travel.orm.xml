<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">
   <entity name="App\Domain\Travel\Model" table="travel">
     <id name="id" column="id" type="int">
         <generator strategy="AUTO"/>
     </id>
     <field name="createdAt" type="dateTime" column="created_at"/>
     <field name="updatedAt" type="dateTime" column="updated_at"/>
     <field name="title" length="255" column="title"/>
     <field name="slug" length="255" column="slug"/>
     <field name="photo" length="255" column="photo"/>
     <field name="lat" type="decimal" precision="10" scale="5" column="lat"/>
     <field name="lng" type="decimal" precision="10" scale="5" column="lat"/>
     <field name="lat0" type="decimal" precision="10" scale="5" column="lat"/>
     <field name="lng0" type="decimal" precision="10" scale="5" column="lat"/>
     <field name="lat1" type="decimal" precision="10" scale="5" column="lat"/>
     <field name="lng1" type="decimal" precision="10" scale="5" column="lat"/>
     <field name="startAt" type="dateTime" column="start_at"/>
     <field name="endAt" type="dateTime" column="end_at"/>
     <field name="description" type="text" column="description"/>
   </entity>

