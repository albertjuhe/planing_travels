<div class="row">
    <div class="col-sm-4">
        <h3>{{travel.title }}</h3>
        <h4>{{travel.description }}</h4>
    </div>
    <div class="col-sm-4">
        <h4>from {{ travel.startAt|date("m/d/Y") }} to {{ travel.endAt|date("m/d/Y") }}</h4>

        <h3>Travel made by: {{ travel.user.firstName }} {{ travel.user.lastname }} ({{ mode }})</h3>
        <div id="panel-information"></div>
    </div>
    <div class="col-sm-4">
        <span class="glyphicon glyphicon-star" aria-hidden="true"></span> Likes {{ travel.stars }}
        <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> Watch {{ travel.watch }}
    </div>
</div>